flowchart
@startuml
start

:Login Pengguna;
if (Hak Akses Admin?) then (Ya)
  :Kelola Data Fakultas;
  :Upload / Update Data;
endif

:Dashboard Tematik;
repeat
  :Pilih Analisis (Mahasiswa/Dosen/IPK/etc);
  :Tampilkan Grafik & Statistik;
  if (Export Laporan?) then (Ya)
    :Export ke PDF / Excel;
  endif
repeat while (Ada Analisis Lain?)

:Logout;

stop
@enduml


